<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.budgetapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GraphSummary"></activity>
        <activity android:name=".Start" />
        <activity android:name=".Income" />
        <activity android:name=".Overview" />
        <activity android:name=".OtherExpenses" />
        <activity android:name=".Entertainment" />
        <activity android:name=".Transportation" />
        <activity android:name=".Home" />
        <activity android:name=".CalendarPage" />
        <activity android:name=".Plan" />
        <activity android:name=".Food" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>